<UserControl x:Class="realestatefinder.EmailTextFiledPlusSave"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:realestatefinder"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <Button Name="SaveEmail" Content="Save" Margin="4" DockPanel.Dock="Right"></Button>
        <local:EmailTextField x:Name="EmailTextField" DockPanel.Dock="Left"></local:EmailTextField>
    </DockPanel>
</UserControl>
