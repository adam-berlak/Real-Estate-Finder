<StackPanel x:Class="realestatefinder.ViewSavedSearchesPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:realestatefinder"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <Label Content="Choose a previously saved search to load"></Label>
    <ItemsControl Name="SavedSearches"></ItemsControl>
    <Button Content="Cancel" Margin="4" Click="Cancel_Click"></Button>
    <Separator></Separator>
</StackPanel>
